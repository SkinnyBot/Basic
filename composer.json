{
    "name": "skinnybot/basic",
    "description": "A simple plugin for Skinny.",
    "homepage": "https://github.com/SkinnyBot/Basic",
    "keywords": ["discord", "bot", "php", "discordphp", "botphp", "skinny", "plugin", "basic"],
    "type": "skinny-plugin",
    "license": "MIT",
    "support": {
        "source":"https://github.com/SkinnyBot/Basic",
        "issues":"https://github.com/SkinnyBot/Basic/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=5.6",
        "skinnybot/skinny": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "3.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Basic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BasicTest\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
